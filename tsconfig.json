{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "lib",
		"lib": ["es6"],
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"strict": true,
		"noUnusedLocals": true,
		"noImplicitAny": true,
	},
	"exclude": [
		"node_modules",
    "test"
	]
}