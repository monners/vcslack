{
  "name": "vcslack",
  "displayName": "VCSlack",
  "description": "send snippet or text to slack",
  "version": "1.11.0",
  "publisher": "verydanny",
  "homepage": "https://github.com/verydanny/vcslack/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/verydanny/vcslack"
  },
  "bugs": {
    "url": "https://github.com/verydanny/vcslack/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.35.0"
  },
  "icon": "image/icon.png",
  "categories": [
    "Other"
  ],
  "author": {
    "name": "Daniil Veremchuk",
    "email": "daniel@verydanny.com",
    "url": "https://github.com/verydanny/"
  },
  "keywords": [
    "Slack",
    "chat",
    "communication",
    "collaboration",
    "messenger",
    "slack snippets",
    "snippets"
  ],
  "activationEvents": [
    "onCommand:vcslack.sendSnippet"
  ],
  "files": [
    "./lib"
  ],
  "main": "./lib/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vcslack.sendSnippet",
        "title": "VCSlack: Send Snippet"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VCSlack configuration",
      "properties": {
        "vcslack.selfToken": {
          "type": "array",
          "default": [
            "your slack token 1",
            "your slack token 2"
          ],
          "description": "Specify the tokens of your slack team"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.137",
    "@types/mocha": "^5.2.7",
    "@types/request": "^2.48.2",
    "@types/request-promise": "^4.1.44",
    "mocha": "^6.2.0",
    "typescript": "^3.5.3",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "rambda": "^3.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
