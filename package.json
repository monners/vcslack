{
  "name": "vcslack",
  "displayName": "VCSSlack",
  "description": "send snippet or text to slack",
  "version": "0.0.10",
  "publisher": "mal097",
  "homepage": "https://github.com/mal097/vslack/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mal097/vslack"
  },
  "bugs": {
    "url": "https://github.com/mal097/vslack/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^0.10.6"
  },
  "icon": "image/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.VCSlack"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.VCSlack",
        "title": "VCSlack",
        "description": "send text to slack as snippet or plane text."
      }
    ],
    "configuration": {
      "type": "object",
      "title": "VCSlack configuration",
      "properties": {
        "vcslack.selfToken": {
          "type": "array",
          "default": [
            "your slack token 1",
            "your slack token 2"
          ],
          "description": "Specify the tokens of your slack team"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "@types/request": "^2.47.1",
    "intern": "^4.2.2",
    "mocha": "^5.2.0",
    "remap-istanbul": "^0.11.1",
    "typescript": "^2.9.2",
    "vscode": "^1.1.18"
  }
}
